{
  "name": "node.js_course_practice",
  "version": "1.0.0",
  "description": "Node.js practice project.",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "start": "node .",
    "start:dev": "nodemon --watch src",
    "build": "rimraf ./build && tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "babel": true,
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "collectCoverage": true,
    "collectCoverageFrom": ["./src/**"],
     "coverageReporters": ["json", "html"],
    "testTimeout": 2000
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/my-projects192/node.js_course_practice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/my-projects192/node.js_course_practice/issues"
  },
  "homepage": "https://gitlab.com/my-projects192/node.js_course_practice#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "express-validator": "^7.0.1",
    "joi": "^17.11.0",
    "mongoose": "^7.6.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.8.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
